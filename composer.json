{
	"name": "oktopost/se-taco",
    "description": "Wrapper for the package facebook/webdriver",
	"type": "library",
	"license": "MIT",
	"homepage": "https://github.com/Oktopost/SeTaco",
	"authors":
	[
		{
			"name": "Alexey Puchkov",
			"email": "alexey@oktopost.com",
			"role": "Developer"
		},
		{
			"name": "Daniel Podrabinek",
			"email": "dpodrabinek@oktopost.com",
			"role": "Developer"
		}
	],
	"minimum-stability": "stable",
	"autoload":
	{
		"psr-4": 
		{
			"SeTaco\\": "Source/"
		}
	},
	"require": 
	{
		"unstable-cacao/traitor": "^1.0",
		"unstable-cacao/structura": "^2.1.0",
		"oktopost/objection": "^1.0",
		"oktopost/file-system": "^0.0",
		"facebook/webdriver": "^1.6",
		"oktopost/cartograph": "^1.1"
	},
	"require-dev":
	{
		"phpunit/phpunit": "^7.2",
		"php-coveralls/php-coveralls": "^2.1"
	},
    "scripts": 
	{
		"test": "./vendor/phpunit/phpunit/phpunit --dont-report-useless-tests",
		"test-cover": "./vendor/phpunit/phpunit/phpunit --dont-report-useless-tests --coverage-html ./build/cover"
    }
}
